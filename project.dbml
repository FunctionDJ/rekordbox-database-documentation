    rb_local_data_status [name: "djmd_artist_rb_local_data_status"]
    rb_local_deleted [name: "djmd_artist_rb_local_deleted"]
    (rb_local_usn, ID) [name: "djmd_artist_rb_local_usn__i_d"]
  }
}

